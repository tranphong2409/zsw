<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:u="sap.ui.unified"
		controllerName="sap.ui.project.e2etm.controller.RoomRents" xmlns:html="http://www.w3.org/1999/xhtml">
<Panel headerText="Upload">
		<content>
			<VBox>
				<HBox justifyContent="End">
					<Link text="Download Template" press="onDownloadTemplate" />
				</HBox>
				<HBox width="500px" justifyContent="SpaceBetween" alignItems="Center">
					<Label text="Filename:" design="Bold" />
					<u:FileUploader id="fileUploader" name="myFileUpload"

						tooltip="Upload your file to the local server" />
				</HBox>
			   <Button text="Upload File" press="handleUploadPress" />
			</VBox>
		</content>
	</Panel>
</core:View>